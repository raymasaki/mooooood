<div class="mood <%= mood.emotion %> wow fadeInUpBig" data-hour="<%= mood.created_at.strftime("%H").to_i %>" data-min="<%= mood.created_at.strftime("%M").to_i %>">
  <div class="circle"></div>
  <p><%= mood.user.username.capitalize %> felt <%= mood.emotion %><br>
  <span class="timestamp">at <%= (mood.created_at).strftime("%I:%M %p") %></span></p>
</div>
